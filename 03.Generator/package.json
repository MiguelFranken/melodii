{
  "name": "03.tonegenerator",
  "version": "1.0.0",
  "description": "Tone Generator for Media Computing Project",
  "main": "src/index.ts",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "npm run build:express && npm run build:tonejs:ci",
    "build:dev": "npm run build:express && npm run build:tonejs:dev",
    "build:express": "tsc --p tsconfig.json",
    "build:tonejs:ci": "webpack --env.production --config ./webpack.config.js && cp -r ./src/static/* ./dist/static",
    "build:tonejs:dev": "webpack --env.development --config ./webpack.config.js && cp -r ./src/static/* ./dist/static",
    "start": "node dist/index.js",
    "start:dev": "npm run build:dev && node dist/index.js",
    "lint": "tslint -- src/*.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/logger": "^1.1.9",
    "@tonaljs/tonal": "^3.2.2",
    "express": "^4.17.1",
    "socket.io-client": "^2.3.0",
    "tonal": "^2.2.2",
    "tone": "^13.8.25",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.32",
    "ts-node-dev": "^1.0.0-pre.43",
    "@types/express": "^4.17.1",
    "@types/node": "^12.11.1",
    "webpack-node-externals": "^1.7.2",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
